{
  "name": "relay-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn run relay && next dev",
    "build": "yarn run relay && next build",
    "start": "yarn run relay && next start",
    "lint": "next lint",
    "relay": "relay-compiler"
  },
  "relay": {
    "artifactDirectory": "./src/__generated__",
    "src": "./src/",
    "schema": "./schema.graphql",
    "language": "typescript"
  },
  "dependencies": {
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-relay": "^14.1.0",
    "relay-runtime": "^14.1.0"
  },
  "devDependencies": {
    "@types/node": "18.6.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-relay": "^13.0.2",
    "@types/relay-runtime": "^13.0.3",
    "babel-plugin-relay": "^14.1.0",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.3",
    "graphql": "^16.5.0",
    "relay-compiler": "^14.1.0",
    "typescript": "4.7.4"
  }
}
